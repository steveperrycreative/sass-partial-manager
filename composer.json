{
  "name": "steveperrycreative/sass-partial-manager",
  "description": "A quick way to manage Sass partials.",
  "keywords": [
    "Sass",
    "CSS",
    "Partials"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Steve Perry",
      "email": "ideas@steveperrycreative.com"
    }
  ],
  "require": {
    "symfony/console": "^3.0.0"
  },
  "autoload": {
    "psr-4": {
      "Spc\\": "src"
    }
  },
  "bin": [
    "spm"
  ],
  "spm_config": {
    "sass_directory": "scss",
    "stylesheet_name": "styles.scss"
  }
}
